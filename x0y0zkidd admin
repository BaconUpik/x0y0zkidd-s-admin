local function update()
	if me.Character then
		hum = me.Character:WaitForChild("Humanoid")
		hrp = me.Character:WaitForChild("HumanoidRootPart")
	end
end
 
me.CharacterAdded:Connect(update)
if me.Character then
	update()
end
 
